{
  "name": "dcfs-frontend",
  "version": "0.0.1",
  "description": "Frontend for distributed cloud file system",
  "productName": "DCFS Frontend",
  "author": "RazorMeister",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "coverage": "vitest run --coverage",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vueuse/core": "^9.4.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "deep-object-diff": "^1.1.7",
    "mitt": "^3.0.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^2.0.5",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "jest-extended": "^3.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "quasar-app-extension-vuelidate-rules": "^0.5.1",
    "vitest": "^0.15.0",
    "vitest-localstorage-mock": "^0.0.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
